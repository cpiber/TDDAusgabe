{
  "name": "tddausgabe",
  "version": "2.0.4",
  "description": "Ausgabeprogramm f√ºr Tischlein Deck Dich",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run build:php",
    "build:js": "rollup -c",
    "build:css": "sass src/files/client.scss build/tmp/client.css --style=compressed",
    "build:php": "phpinclude -c phpinclude.js src/TDD_Ausgabe.php build/TDD_Ausgabe.php",
    "start": "NODE_ENV=development concurrently \"npm:start:js\" \"npm:start:css\" \"npm:start:php\"",
    "start:js": "rollup -c -w",
    "start:css": "sass src/files/client.scss build/tmp/client.css -w",
    "start:php": "phpinclude -c phpinclude.js -w src/TDD_Ausgabe.php build/TDD_Ausgabe.php"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jquery": "^3.5.16",
    "concurrently": "^8.0.1",
    "core-js-pure": "^3.30.2",
    "jquery": "^3.6.4",
    "jsbarcode": "^3.11.5",
    "object.values": "^1.1.6",
    "phpinclude": "https://github.com/cpiber/PHPInclude/releases/download/v2.0.0/phpinclude-2.0.0.tgz",
    "rollup": "^3.20.4",
    "sass": "^1.62.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cpiber/TDDAusgabe.git"
  },
